[program:SiteGroupMember]
;要执行的命令
command=/websites/sitegroupmember/SiteGroupMember 
;命令执行的目录 
directory=/website/sitegroupmember/ 
;环境变量
environment=ASPNETCORE__ENVIRONMENT=Production
;进程执行的用户身份
user=root
;是否自动启动
autostart=true
;是否自动重启
autorestart=true
;自动重启间隔
startsecs=1
;标准错误日志
stderr_logfile=/var/log/SiteGroupMemberApp.err.log
;标准输出日志
stdout_logfile=/var/log/SiteGroupMemberApp.out.log
stopsignal=INT
